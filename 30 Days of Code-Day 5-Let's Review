#include <cmath>
#include <cstddef>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() 
{
    int t;
    string str,str2,str3;
    cin>>t;
    for(int i=0;i<t;i++)
    {
        str2="";
        str3="";
        cin>>str;
        for(int j=0;j<str.size();j++)
        {
            if(j%2==0)
            str2.push_back(str[j]);
            if(j%2!=0)
            str3.push_back(str[j]);
        }
        cout<<str2<<" "<<str3<<endl;
    }
    return 0;
}
