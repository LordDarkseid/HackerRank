int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int a,q;
    cin>>a>>q;
    vector <int> vec[a];
    for(int i=0;i<a;i++)
    {
        int size;
        cin>>size;
        int b;
        for(int j=0;j<size;j++)
        {
            cin>>b;
            vec[i].push_back(b) ;    
        }
    }
    int c,d;
    for(int k=0;k<q;k++)
    {
        cin>>c>>d;
        cout<<vec[c][d]<<endl;
    }  
    return 0;
}
